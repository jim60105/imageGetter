{
  "manifest_version": 2,

  "name": "imageGetter",
  "description": "Get the direct original link in search pages.",
  "version": "1.0",
  "author": "jim60105",
  "background": { "scripts": ["background.js"] },
  "content_scripts":[{
    "matches":["https://chan.sankakucomplex.com/*"],
    "js":["jquery-2.2.0.min.js","content_script.js"]
  }],
  "page_action": {
    "default_icon": "icon32.png"
  },
  "icons": {
    "16": "icon16.png",
    "32": "icon32.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": [
    "tabs",
    "activeTab"
  ]
}
